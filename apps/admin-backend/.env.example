# Admin backend
NODE_ENV=production
PORT=4100
APP_URL=https://admin-api.gptishka.shop
ADMIN_UI_URL=https://gptishka.shop
DATABASE_URL=postgresql://db_user:db_password@127.0.0.1:5432/gptishka_admin?schema=public
JWT_SECRET=replace_me_single_jwt_secret_or_use_access_refresh_below
JWT_ACCESS_SECRET=replace_me_access
JWT_REFRESH_SECRET=replace_me_refresh
JWT_ACCESS_TTL=15m
JWT_REFRESH_TTL_DAYS=30
REFRESH_COOKIE_NAME=admin_refresh_token
BCRYPT_ROUNDS=12
SEED_CREATE_DEFAULT_USERS=false
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX=120
AUTH_RATE_LIMIT_MAX=10
SMTP_HOST=
SMTP_PORT=587
SMTP_SECURE=false
SMTP_USER=
SMTP_PASSWORD=
SMTP_FROM=info@gptishka.shop
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=
FX_USD_RUB=95
FX_EUR_RUB=103
FX_USDT_RUB=95
PAYMENT_PROVIDER=gateway
# Enot API key (x-api-key)
PAYMENT_SECRET=replace_me_enot_api_key
PAYMENT_SHOP_ID=replace_me_shop_id
# Enot additional key for webhook HMAC signature
WEBHOOK_SECRET=replace_me_enot_webhook_secret
PAYMENT_API_BASE_URL=https://api.enot.io
PAYMENT_CREATE_PATH=/invoice/create
PAYMENT_REFUND_PATH=/invoice/refund
PAYMENT_SUCCESS_URL=https://gptishka.shop/success.html
PAYMENT_FAIL_URL=https://gptishka.shop/fail.html
PAYMENT_WEBHOOK_URL=https://admin-api.gptishka.shop/api/public/webhook/payment
PAYMENT_WEBHOOK_SIGNATURE_HEADER=x-api-sha256-signature
PAYMENT_WEBHOOK_IP_ALLOWLIST=
STORAGE_DRIVER=local
